{
    "name": "thehustle/silverstripe-blocks",
    "description": "A SilverStripe module to create couple of custom multiple blocks.",
    "type": "silverstripe-vendormodule",
    "require": {
        "php": "^7.1 || ^8.0",
        "silverstripe/cms": "^4 || ^5",
        "silverstripe/framework": "^4 || ^5",
        "dnadesign/silverstripe-elemental": "^4 || ^5"
    },
    "autoload": {
        "psr-4": {
            "TheHustle\\": "src/"
        }
    },
    "extra": {
        "expose": [
            "templates",
            "client"
        ]
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Billy Joel Ranario",
            "email": "billyranario@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
